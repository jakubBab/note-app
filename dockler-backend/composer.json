{
    "name": "symfony/skeleton",
    "type": "project",
    "license": "MIT",
    "description": "A minimal Symfony project recommended to create bare bones applications",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "^1.0@dev",
        "doctrine/annotations": "1.13.x-dev",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "3.1.x-dev",
        "doctrine/orm": "2.9.x-dev",
        "jms/serializer-bundle": "3.9.x-dev",
        "lexik/jwt-authentication-bundle": "2.x-dev",
        "nelmio/cors-bundle": "^2.1",
        "ramsey/uuid": "4.x-dev",
        "stof/doctrine-extensions-bundle": "1.6.x-dev",
        "symfony/console": "*",
        "symfony/dotenv": "*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "*",
        "symfony/messenger": "5.*",
        "symfony/monolog-bundle": "3.x-dev",
        "symfony/proxy-manager-bridge": "5.*",
        "symfony/runtime": "*",
        "symfony/translation": "5.*",
        "symfony/validator": "5.*",
        "symfony/yaml": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "3.5.x-dev",
        "symfony/maker-bundle": "^1.30",
        "symplify/easy-coding-standard": "9.3.x-dev"
    }
}
